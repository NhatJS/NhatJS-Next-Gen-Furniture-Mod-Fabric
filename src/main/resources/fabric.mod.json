{
	"schemaVersion": 1,
	"id": "nextgen_furniture",
	"version": "${version}",
	"name": "NhatJS's Next-Gen Furniture Mod",
	"description": "The new generation of Furniture Mod comes with minimalistic and realistic features. Furniture Mod thế hệ tiếp theo đi kèm với các tính năng tối giản và thực tế.",
	"authors": [
		"NhatJS (aka James Southfall)"
	],
	"contact": {
		"homepage": "https://www.youtube.com/channel/UCWZnMnD4WqJgJ-StYyd_SQw",
		"sources": "https://github.com/NhatJS/NhatJS-Next-Gen-Furniture-Mod-Fabric"
	},
	"license": "MIT",
	"icon": "assets/nextgen_furniture/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.nhatjs.nextgen_furniture.NhatJSNextGenFurnitureMod"
		],
		"fabric-datagen": [
			"net.nhatjs.nextgen_furniture.NhatJSNextGenFurnitureModDataGenerator"
		],
		"client": [
			"net.nhatjs.nextgen_furniture.NhatJSNextGenFurnitureModClient"
		]
	},
	"mixins": [
		"nhatjs-next-gen-furniture-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"framework": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}